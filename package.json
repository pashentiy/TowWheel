{
  "name": "towWheel",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "pods": "cd ios && pod install",
    "android": "source ~/.bash_profile && react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "both": "react-native run-ios && react-native run-android",
    "fd": "cd server/functions && firebase deploy --only functions",
    "localserver": "cd server/functions && firebase serve",
    "beta": "cd ios && bundle exec fastlane beta",
    "release": "cd ios && bundle exec fastlane release",
    "cp-ios": "code-push release-react CallOn ios -d Production",
    "cp-android": "code-push release-react CallOn-Android android -d Production"
  },
  "dependencies": {
    "@react-native-community/masked-view": "^0.1.10",
    "@react-navigation/drawer": "^5.11.2",
    "@react-navigation/native": "^5.8.7",
    "@react-navigation/stack": "^5.12.5",
    "formik": "^2.2.3",
    "lottie-ios": "3.1.8",
    "lottie-react-native": "^3.5.0",
    "react": "16.13.1",
    "react-native": "0.63.3",
    "react-native-gesture-handler": "^1.8.0",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-maps": "0.27.1",
    "react-native-permissions": "^3.0.0-beta.2",
    "react-native-reanimated": "^1.13.1",
    "react-native-safe-area-context": "^3.1.8",
    "react-native-screens": "^2.13.0",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "babel-jest": "^25.1.0",
    "eslint": "^6.5.1",
    "jest": "^25.1.0",
    "metro-react-native-babel-preset": "^0.59.0",
    "react-test-renderer": "16.13.1"
  },
  "jest": {
    "preset": "react-native"
  }
}
